{
  "version": 2,
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "outputDirectory": "dist"
  },
  "routes": [
    {
      "src": "/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "/dist/app.js"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs14.x"
    }
  }
}
